<?xml version="1.0" encoding="UTF-8"?>
<table sqlname="TASK">

    <operation name="selectById" paramtype="parameterObject">
        <sql>
            select * FROM TASK
            WHERE ID = #id#
        </sql>

    </operation>


            <operation name="insert" paramtype="object">
                <sql>
                    INSERT INTO
                    HOTEL_ORDER(
                    PARENTID ,
                    NAME ,
                    DESCRIPTION,
                    CORN ,
                    STATE ,
                    CREATETIME

                    )
                    VALUES(
                    #parentId#,
                    #name#,
                    #description#,
                    #cityId#,
                    #corn#,
